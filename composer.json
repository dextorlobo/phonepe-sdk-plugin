{
    "name": "imarun/phone-pay-sdk-plugin",
    "description": "PhonePe SDK plugin",
    "type": "wordpress-plugin",
    "autoload": {
        "psr-4": {
            "Imarun\\PhonePaySdkPlugin\\": "includes/"
        }
    },
    "authors": [
        {
            "name": "Arun Sharma",
            "email": "dextorlobo@gmail.com"
        }
    ],
    "require": {
        "phonepe/phonepe-pg-php-sdk": "^1.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": [
                {
                    "dist": {
                        "type": "zip",
                        "url": "https://phonepe.mycloudrepo.io/public/repositories/phonepe-pg-php-sdk/phonepe-pg-php-sdk.zip"
                    },
                    "name": "phonepe/phonepe-pg-php-sdk",
                    "version": "1.0.0",
                    "autoload": {
                        "classmap": ["/"]
                    }
                }
            ]
        }
    ]
}
